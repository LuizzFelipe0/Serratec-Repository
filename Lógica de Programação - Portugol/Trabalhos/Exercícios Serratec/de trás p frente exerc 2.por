programa
{                      
	
	funcao inicio()
	{
		inteiro n[10]

		para(inteiro i=9; i >= 0; i--){
			escreva("Digite um número: ", "\n")
			leia(n[i])

		}

		para(inteiro i=0; i <= 9; i++){
			escreva("Os números são: ", n[i], "\n")

		}
	}
}
